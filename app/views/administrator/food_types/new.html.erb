<br />
<div class="booyah-box col-10 offset-1">
	<h1>Add Nutritional Info. Here:</h1>

	<%= simple_form_for @food_type, url: administrator_food_types_path do |f| %>
    <%= f.input :name %>
    <%= f.input :food_id, collection: Food.all, as: :grouped_select, group_method: :foods, prompt: "Select One" %>
     <br />
    <%= f.submit "Submit", class: 'btn btn-primary' %>
    <br />
	<% end %>
  <br />

</div>